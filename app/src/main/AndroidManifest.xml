<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dong.gamehit">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/index"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/index"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".gameover" />
        <activity android:name=".hitting" />
        <activity android:name=".rankscene" />
        <activity android:name=".DoubleActivity"></activity>
        <activity android:name=".Double2Activity"></activity>
        <activity android:name=".HighScore"></activity>
        <activity android:name=".LowScore"></activity>
    </application>

</manifest>